decl

int a, b, c, d;

enddecl

begin
    read(a);
    write (a);
    read( b);
    read(c );
    read(d);
    a = b+2*d/b;
    if(a == 0) then
        write(a);
    else
        write(a-1);
    endif;

    while(a <= b+1)do
        if(a  < 0) then
            read(b);
        endif;
    endwhile;
    write(b);
    write(c);
    write(d);
    write(a*b*d/c);
end;